             {"name": "LITERAL"},
             {"name": "METHOD_REF"},
             {"name": "RETURN"},
             {"name": "BLOCK"},
             {"name": "CONTROL_STRUCTURE"}
           ]},
           {"edgeName": "RECEIVER", "inNodes": [
